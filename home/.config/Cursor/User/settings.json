{
    "editor.fontFamily": "Fira Code, IntelOne Mono, Consolas, monospace",
    "editor.fontWeight": "400",
    "editor.fontLigatures": true,
    "editor.tabSize": 2,
    "editor.wordWrap": "off",
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "editor.mouseWheelScrollSensitivity": 6,
    "editor.stablePeek": true,
    "editor.minimap.enabled": false,
    "editor.autoIndent": "full",
    "explorer.openEditors.visible": 1,
    "files.associations": {
        "*.txt": "plaintext",
        "*.pug": "jade",
        "*.toml": "ini"
    },
    "files.exclude": {
        "**/node_modules": true,
        "**/.ebextensions": true,
        "**/.elasticbeanstalk": true,
        "**/temp/*/*": true,
        "**/*.gz": true
    },
    "files.eol": "\n",
    "javascript.validate.enable": true,
    "telemetry.telemetryLevel": "off",
    "git.enabled": true,
    "git.autofetch": false,
    "git.enableSmartCommit": true,
    "git.checkoutType": [
        "local"
    ],
    "gitProjectManager.baseProjectsFolders": [
        "/home/flesler/Code",
    ],
    "gitProjectManager.storeRepositoriesBetweenSessions": true,
    "gitProjectManager.maxDepthRecursion": 2,
    "gitProjectManager.checkRemoteOrigin": false,
    "window.newWindowDimensions": "inherit",
    "extensions.autoUpdate": false,
    "workbench.colorTheme": "Monokai",
    "workbench.startupEditor": "none",
    "explorer.confirmDragAndDrop": false,
    "breadcrumbs.enabled": true,
    "editor.tabCompletion": "on",
    "editor.dragAndDrop": false,
    "editor.suggest.localityBonus": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "search.smartCase": true,
    "http.proxyStrictSSL": false,
    "npm.runSilent": true,
    "git.rebaseWhenSync": true,
    "git.allowForcePush": true,
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "workbench.list.defaultFindMode": "filter",
    "workbench.quickOpen.closeOnFocusLost": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "search.exclude": {
        "**/.git": true,
        "src/db/migrations": true
    },
    "search.showLineNumbers": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 8,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.wordWrapColumn": 120,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "eslint.format.enable": true,
    "eslint.lintTask.enable": true,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "javascript.format.semicolons": "remove",
    "typescript.format.semicolons": "remove",
    "git.autoStash": true,
    "typescript.tsdk": "node_modules/typescript/lib",
    "git.enableCommitSigning": true,
    "git.ignoredRepositories": [
        "/etc"
    ],
    "git.inputValidationLength": 100,
    "git.inputValidationSubjectLength": 100,
    "git.confirmSync": false,
    "files.autoSave": "off",
    "search.quickOpen.history.filterSortOrder": "recency",
    "search.sortOrder": "modified",
    "editor.formatOnSaveMode": "modifications",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.importModuleSpecifierEnding": "minimal",
    "typescript.surveys.enabled": false,
    "eslint.run": "onSave",
    "git.fetchOnPull": true,
    "git.allowNoVerifyCommit": true,
    "emmet.excludeLanguages": [
        "typescript",
        "markdown"
    ],
    "emmet.showExpandedAbbreviation": "never",
    "editor.formatOnType": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.guides.bracketPairs": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.matchBrackets": "near",
    "editor.guides.bracketPairsHorizontal": false,
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "security.workspace.trust.enabled": false,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.guides.indentation": true,
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "eslint.validate": [
        "typescript",
        "typescriptreact"
    ],
    "typescript.validate.enable": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "notebook.diff.enablePreview": false,
    "git.mergeEditor": false,
    "editor.folding": false,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "git.openRepositoryInParentFolders": "never",
    "aws.telemetry": false,
    "workbench.editor.empty.hint": "hidden",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "diffEditor.experimental.showMoves": true,
    "diffEditor.codeLens": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.autoClosingComments": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingBrackets": "never",
    "cursor.chat.defaultNoContext": true,
    "cursor.chat.premiumChatAutoScrollWhenAtBottom": true,
    "update.mode": "manual",
    "diffEditor.ignoreTrimWhitespace": false,
    "window.customTitleBarVisibility": "auto",
    "cursor.cmdk.useThemedDiffBackground": true,
    "cursor.composer.cmdPFilePicker2": true,
    "cursor.cpp.disabledLanguages": [
      "CSV",
      "TSV",
      "Plain Text",
      "Text Plain",
      "Log",
      "log",
      "properties",
      "untitled"
    ],
    "cursor.composer.shouldShowMarkdownHoverParticipantActions": false,
    "workbench.editor.enablePreview": false,
    "git.confirmForcePush": false,
    "[typescript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "cursor.composer.collapsePaneInputBoxPills": true,
    "cursor.composer.renderPillsInsteadOfBlocks": true,
    "cursor.composer.shouldChimeAfterChatFinishes": true,
    "diffEditor.renderSideBySide": false,
    "terminal.integrated.defaultProfile.linux": "bash",
    "workbench.editorAssociations": {
      "*.mdc": "default"
    },
    "cursor.composer.backspaceRemoveContext": false,
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "json.schemaDownload.enable": true,
    "window.confirmSaveUntitledWorkspace": false
}